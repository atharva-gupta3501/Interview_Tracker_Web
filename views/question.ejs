<link rel="stylesheet" href="/stylesBlog.css">
<%- include('partials/nav'); -%>
<main id="main">

  <!-- ======= Breadcrumbs ======= -->
  <section class="breadcrumbs">
    <div class="container">

      <div class="d-flex justify-content-between align-items-center">
        <h2>
          <%= title%>
        </h2>
        <ol>
          <li><a href="/">Home</a></li>
          <li>
            <%= title%>
          </li>
        </ol>
      </div>

    </div>
  </section><!-- End Breadcrumbs -->

  <section class="inner-page">
